{
	"compilerOptions": {
		"types": ["bun-types"],
		"moduleResolution": "bundler",
		"outDir": "./build",
		"lib": ["esnext"],
		"module": "esnext",
		"target": "esnext",
		"esModuleInterop": true,
		"rootDir": "src",
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
